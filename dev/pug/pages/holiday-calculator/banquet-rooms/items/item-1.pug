li.calculator-rooms__item
	.calculator-rooms__inner
		picture.calculator-rooms__picture
			source(media="(min-width: 1440px)" srcset="static/images/content/birthday/banquet-1-large.jpg")
			source(media="(min-width: 1024px)" srcset="static/images/content/birthday/banquet-1-desktop.jpg")
			source(media="(min-width: 768px)" srcset="static/images/content/birthday/banquet-1-tablet.jpg")
			img.calculator-rooms__img(src="static/images/content/birthday/banquet-1-mobile.jpg" width="320" height="320" alt="Картинка банкетного зала")
		.calculator-rooms__box
			h3.calculator-rooms__heading.font-h4 Страна чудес
			p.calculator-rooms__text до 15 человек
			ul.calculator-rooms__radio.radio-times
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderlandTimes" id="calcWonderlandTimes9AM" value="9AM" disabled)
					label.radio-times__label(for="calcWonderlandTimes9AM") 9:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderlandTimes" id="calcWonderlandTimes11AM" value="11AM")
					label.radio-times__label(for="calcWonderlandTimes11AM") 11:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderlandTimes" id="calcWonderlandTimes13PM" value="13PM" disabled)
					label.radio-times__label(for="calcWonderlandTimes13PM") 13:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderlandTimes" id="calcWonderlandTimes17PM" value="17PM")
					label.radio-times__label(for="calcWonderlandTimes17PM") 17:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderlandTimes" id="calcWonderlandTimes19PM" value="19PM")
					label.radio-times__label(for="calcWonderlandTimes19PM") 19:00
		a.calculator-rooms__shadow-link._popup-link(href="#calc-banquet")
