li.calculator-rooms__item.calculator-rooms__item--disabled
	.calculator-rooms__inner
		picture.calculator-rooms__picture
			source(media="(min-width: 1440px)" srcset="static/images/content/birthday/banquet-3-large.jpg")
			img.calculator-rooms__img(src="static/images/content/birthday/banquet-3-desktop.jpg" width="320" height="320" alt="Картинка банкетного зала")
		.calculator-rooms__box
			h3.calculator-rooms__heading.font-h4 Страна чудес
			p.calculator-rooms__text до 15 человек
			ul.calculator-rooms__radio.radio-times
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderland4Times" id="calcWonderland4Times9AM" value="9AM")
					label.radio-times__label(for="calcWonderland4Times9AM") 9:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderland4Times" id="calcWonderland4Times11AM" value="11AM" disabled)
					label.radio-times__label(for="calcWonderland4Times11AM") 11:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderland4Times" id="calcWonderland4Times13PM" value="13PM" disabled)
					label.radio-times__label(for="calcWonderland4Times13PM") 13:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderland4Times" id="calcWonderland4Times17PM" value="17PM")
					label.radio-times__label(for="calcWonderland4Times17PM") 17:00
				li.radio-times__item
					input.radio-times__input(type="radio" name="calcWonderland4Times" id="calcWonderland4Times19PM" value="19PM")
					label.radio-times__label(for="calcWonderland4Times19PM") 19:00
		a.calculator-rooms__shadow-link._popup-link(href="#calc-banquet")
